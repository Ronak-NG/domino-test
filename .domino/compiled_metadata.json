        "input_schema": {
            "description": "Sleep Piece Input Model",
            "properties": {
                "frequency": {
                    "default": 5,
                    "description": "Frequency of the sine wave in Hz",
                    "title": "Frequency",
                    "type": "integer"
                },
                "duration": {
                    "default": 1,
                    "description": "Duration of the sine wave signal in seconds",
                    "title": "Duration",
                    "type": "integer"
                },
                "sample_rate": {
                    "default": 1000,
                    "description": "Sample rate of the sine wave signal.",
                    "title": "Sample Rate",
                    "type": "integer"
                }
            },
            "title": "InputModel",
            "type": "object"
        },
        "output_schema": {
            "description": "Sleep Piece Output Model",
            "properties": {
                "message": {
                    "description": "Sleep piece executed",
                    "title": "Message",
                    "type": "string"
                }
            },
            "required": [
                "message"
            ],
            "title": "OutputModel",
            "type": "object"
        },
        "secrets_schema": null,
        "source_url": "https://github.com/Ronak-NG/domino-test/tree/main/pieces/ExampleSineWavePiece"
    },
